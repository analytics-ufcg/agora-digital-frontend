{
  "name": "agora-digital-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test_all": "vue-cli-service lint && vue-cli-service test:e2e --headless",
    "cy:verify": "cypress verify",
    "docker_test": "export GUI=false; docker-compose -f docker_extra/test.yml up --build --exit-code-from frontend_test",
    "docker_test_gui": "export GUI=true; docker-compose -f docker_extra/test.yml up --build --exit-code-from frontend_test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix"
    }
  },
  "dependencies": {
    "element-ui": "^2.4.6",
    "loadash": "^1.0.0",
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-mocha": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-standard": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "eslint-plugin-cypress": "^2.0.1",
    "husky": "^1.1.2",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.17",
    "vuex-rest-api": "^2.10.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "vue": {
    "devServer": {
      "disableHostCheck": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
